// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("MONGO_DSN")
}

model Webhook {
  id      String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  address String
  topic   String
}
